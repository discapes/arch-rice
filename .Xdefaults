URxvt.intensityStyles:	true
!URxvt.background:       #000000
URxvt.depth:		32
urxvt*font:             xft:Bitstream Vera Sans Mono:size=12
URxvt.scrollBar:	false
URxvt.borderColor:      #333333
URxvt.internalBorder:   20
URxvt.bell:             false
URxvt.letterSpace: 0
URxvt.lineSpace: 0
#URxvt.cursorUnderline: true
URxvt.cursorBlink:  true
URxvt.saveLines: 1000

URxvt.perl-ext-common: default,matcher,url-select,keyboard-select,resize-font
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.resize-font.smaller: M-j
URxvt.resize-font.bigger: M-k
URxvt.matcher.button: 1


